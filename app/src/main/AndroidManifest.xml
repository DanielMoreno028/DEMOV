<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/apple"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/apple_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MovieAplication"
        tools:targetApi="31">
        <activity
            android:name=".ui.login.Activity_inicio_sesion"
            android:exported="false"
            android:label="@string/title_activity_inicio_sesion" />
        <activity
            android:name=".Activity_home"
            android:exported="false"
            android:label="@string/title_activity_home"
            android:theme="@style/Theme.MovieAplication" />
        <activity
            android:name=".Activity_CrearCuenta"
            android:exported="false" />
        <activity
            android:name=".MainActivity2_login"
            android:exported="false"
            android:label="@string/title_activity_login" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>